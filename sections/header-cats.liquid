}
    .header-cats-main .swiper-slide {
    background-color: #f6f6f4 !important;
    flex-direction: column !important;
}
.header-cats-main .header-cats p {
    font-weight: 600;
    color: #214068;
    margin: 0;
    text-align: center !important;
}

.header-cats-main .header-cats-title {border-right: 1px solid #2140682b;}
.header-cats-main .header-cats-title h2 {
    font-weight: 600;
    font-size: 28px;
    color: #214068;
    margin: 0;
    text-align: center !important;
}
    .text-with-icons__block-icon .icon {
    height: 60px !important;
    width: 60px !important;
    color: #000000;
    fill: #000000;
    display: inline-block;
    height: 20px;
    vertical-align: middle;
    width: 20px;
}
    .swiper-button-next:empty, .swiper-button-prev:empty {
      display: block;
    }
    .header-cats-main .swiper-slide a {
